	@Id
	private int id;

	@OneToMany(targetEntity=PurchaseItem.class, mappedBy="purchase", cascade=CascadeType.MERGE)
	private Set<PurchaseItem> purchaseItems = new HashSet<PurchaseItem>();


	public Purchase() {
